{
  "name": "@kui-shell/plugin-search",
  "version": "0.0.0-semantically-released",
  "description": "KUI plugin for Open Cluster Management Search.",
  "author": "Open Cluster Management",
  "license": "ISC",
  "engines": {
    "npm": ">=6.9.0"
  },
  "keywords": [
    "open cluster management",
    "kui",
    "plugin",
    "search",
    "icp",
    "mcm"
  ],
  "main": "dist/plugin.js",
  "module": "mdist/plugin.js",
  "types": "mdist/plugin.d.ts",
  "private": true,
  "scripts": {
    "buildCSS": "mkdir -p dist/src-web/styles && sass src/src-web/styles/index.scss dist/src-web/styles/index.css && npm run copyCSS",
    "copyCSS": "mkdir -p mdist/src-web/styles && cp -a ./dist/src-web/styles ./mdist/src-web/",
    "commit": "git-cz",
    "compile": "tsc",
    "semantic-release": "semantic-release",
    "setup-dev": "sh build/setup-dev.sh",
    "start": "cd ../plugin-kubectl-boilerplate && npm run start",
    "test:coverage": "mkdir -p test-output && jest --coverage --no-cache",
    "test:unit": "jest --colors",
    "test": "mkdir -p test-output && npx jest --silent",
    "test:watch": "jest --watch",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:chrome": "nightwatch ./build-tools/tests/e2e -e chrome",
    "test:firefox": "nightwatch ./build-tools/tests/e2e -e firefox",
    "test:safari": "nightwatch ./build-tools/tests/e2e -e safari"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "carbon-components": "^10.9.2",
    "carbon-components-react": "^7.9.2",
    "carbon-icons": "^7.0.7",
    "graphql": "^14.5.8",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "nconf": "^0.10.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-tag-autocomplete": "^5.11.2"
  },
  "devDependencies": {
    "@kui-shell/client": "npm:@kui-shell/plugin-client-default@^6.0.13",
    "@kui-shell/core": "^6.0.13",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.8",
    "@types/react": "^16.9.18",
    "@types/react-dom": "^16.9.5",
    "chalk": "^3.0.0",
    "chromedriver": "^79.0.0",
    "commitizen": "^4.0.3",
    "del": "^5.1.0",
    "jest": "^25.1.0",
    "electron": "6.1.5",
    "geckodriver": "^1.19.1",
    "ghooks": "^2.0.4",
    "nightwatch": "^1.3.2",
    "semantic-release": "^16.0.2",
    "xml2json": "^0.12.0",
    "jest-tap-reporter": "^1.9.0",
    "typescript": "^3.7.4",
    "sass": "^1.25.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:open-cluster-management/search-kui-plugin.git"
  },
  "release": {
    "branch": "master",
    "plugins": [
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "./plugin-search.tgz",
              "label": "KUI plugin-search package"
            }
          ]
        }
      ]
    ]
  }
}
