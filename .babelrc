{
  "presets": [[
      "@babel/preset-env", {
        "targets": {
          "node": true
        }
      }],
    "@babel/preset-react"
  ],
  "plugins":[
    "@babel/plugin-transform-modules-commonjs",
    "babel-plugin-dynamic-import-node-babel-7",
    "babel-plugin-ignore-html-and-css-imports"
  ]
}